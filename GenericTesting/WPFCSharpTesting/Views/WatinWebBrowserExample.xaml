<UserControl x:Class="WPFCSharpTesting.Views.WatinWebBrowserExample"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:WPFCSharpTesting.Views"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <UserControl.Resources>
        <ContextMenu x:Key="MnuCustom" StaysOpen="True">
            <MenuItem Header="Print" Command="{Binding PrintPreview}"></MenuItem>
            <MenuItem Header="Custom 1"></MenuItem>
            <MenuItem Header="Custom 3"></MenuItem>
        </ContextMenu>
    </UserControl.Resources>
    <Grid>
        <WebBrowser x:Name="Wb" />
    </Grid>
</UserControl>
