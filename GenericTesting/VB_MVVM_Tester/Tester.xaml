<Window x:Class="Tester"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:VB_MVVM_Tester"
        mc:Ignorable="d"
        Title="MainWindow" Height="100" Width="525">
    <Grid>
    <StackPanel Orientation="Horizontal" Margin="5" Grid.Row="0" >
      <TextBox x:Name="txtAddress" MinWidth="250" Text="{Binding Address}" />
      <Button Content="Map It" IsDefault="True" Command="{Binding GeocodeAddressCommand}"/>
      <TextBox x:Name="txtAddressResult" MinWidth="250" Text="{Binding LocationAddress}" />
    </StackPanel>
  </Grid>
</Window>
