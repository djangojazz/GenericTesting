<Window x:Class="Tester"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:VB_MVVM_Tester"
        mc:Ignorable="d"
        Height="800"
        Width="800"
        Background="{StaticResource brushMainBoard}">
  <Window.Resources>
    <local:DateValueConverter x:Key="DateValueConverter" DateFormat="MM/dd/yy" />
    <local:DecimalConverter x:Key="ConvertDecimal" IncludeComma="True" DecimalPositions="0" />
    
  </Window.Resources>
  <Grid>
    <TextBox></TextBox>
  </Grid>
</Window>