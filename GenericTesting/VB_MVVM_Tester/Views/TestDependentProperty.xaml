<UserControl x:Class="TestDependentProperty"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:VB_MVVM_Tester"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300" Name="L">
    <Grid>
    <TextBlock Text="{Binding ElementName=L, Path=Test}" Height="30" Background="White" Foreground="Black" />
  </Grid>
</UserControl>
